{
  "name": "@quantum-design/vue3-antd-pc-ui",
  "version": "2.0.0-beta.0",
  "description": "vue3,antd组件库",
  "type": "module",
  "main": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "quantum",
    "monorepo",
    "vue3",
    "antd",
    "turborepo"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./es/*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/es/*.js"
    },
    "./lib/*": {
      "types": "./dist/src/*.d.ts",
      "require": "./dist/lib/*.js"
    },
    "./*": "./*"
  },
  "author": "Evan Wu",
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@quantum-design/hooks": "workspace:*",
    "@quantum-design/shared": "workspace:*",
    "@quantum-design/utils": "workspace:*",
    "@quantum-design/vue3-pc-ui": "workspace:*",
    "lodash-es": "~4.17.21",
    "sortablejs": "~1.15.0"
  },
  "devDependencies": {
    "@quantum-design-configs/vite": "workspace:*",
    "@quantum-design/types": "workspace:*",
    "@types/lodash-es": "~4.17.12",
    "sass": "~1.77.6",
    "vue": "~3.4.31",
    "pinia": "~2.2.2",
    "@vue/test-utils": "~2.4.0",
    "@ant-design/icons-vue": "~6.1.0",
    "ant-design-vue": "~4.2.3",
    "vue-router": "~4.4.0",
    "vue-types": "~5.1.1",
    "dayjs": "~1.11.7",
    "csstype": "~3.1.2"
  },
  "peerDependencies": {
    "pinia": "~2.2.2",
    "vue": "~3.4.0",
    "vue-router": "~4.4.0"
  }
}
