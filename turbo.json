{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "build:lib": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**","dist-components/**", ".next/**"]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "test": {
            "dependsOn": ["^build"]
        },
        "test:coverage": {
            "dependsOn": ["^build"],
            "outputs": ["coverage/**"]
        }
    }
}