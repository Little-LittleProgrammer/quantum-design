{
    "name": "qm-monorepo",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "build": "turbo run build --team=\"qm_team\" --token=\"qmfrontTurborepoRemoteCacheToken\" --api=\"http://47.103.85.18:9001\"",
        "build:lib": "turbo run build:lib --team=\"qm_team\" --token=\"qmfrontTurborepoRemoteCacheToken\" --api=\"http://47.103.85.18:9001\"",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "test:coverage": "turbo run test:coverage",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "commit": "czg",
        "prepare": "husky",
        "migrator": "sass-migrator module packages/**/*.scss"
    },
    "devDependencies": {
        "sass-migrator": "catalog:",
        "@changesets/cli": "catalog:",
        "@quantum-design-configs/eslint": "workspace:*",
        "@quantum-design-configs/commitlint": "workspace:*",
        "@quantum-design-configs/tsconfig": "workspace:*",
        "@types/node": "catalog:",
        "@vitest/coverage-v8": "catalog:",
        "@vitest/ui": "catalog:",
        "lint-staged": "catalog:",
        "husky": "catalog:",
        "jsdom": "catalog:",
        "tslib": "catalog:",
        "turbo": "catalog:",
        "typescript": "catalog:",
        "vite": "catalog:",
        "vitest": "catalog:"
    },
    "packageManager": "pnpm@9.15.1",
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=18.0.0"
    },
    "pnpm": {
        "overrides": {
            "eslint": "~8.57.1"
        }
    },
    "volta": {
        "node": "18.18.2",
        "pnpm": "9.15.7"
    }
}
